{
  "trailingComma": "none",
  "overrides": [
    {
      "files": "**/lwc/**/*.html",
      "options": { "parser": "lwc" }
    },
    {
      "files": "*.{cmp,page,component}",
      "options": { "parser": "html","printWidth":120 }
    },
    {
      "files": "*.{cls,trigger,apex}",
      "options": { "parser": "apex","printWidth":120 }
    }
  ]
}
